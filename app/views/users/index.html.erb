<div class="container">
  <h1><%= t(".title") %></h1>
  <ul>
    <% @users.each do |user| %>
      <li>
        <%= user.title %> <%= user.first_name %> <%= user.last_name %> <%= user.birthdate.strftime("%d/%m/%Y") %>
      </li>
    <% end %>
  </ul>
  <%= link_to t(".shared.back"), :back  %>
</div>
